<body>
    <style>
    body {
      background-image: url("/img/listMissions.jpg");
      background-repeat: no-repeat;
      background-attachment: fixed;
      background-position:center center;  
    }
  </style>
  
  {{> header}}

    <h1>Liste</h1>
    <h2>{{ subtitle }}</h2>

    <div id="missionsList" class="w3-container w3-opacity-min"></div>

    <div id="modalBriefing" class="w3-modal"  style="z-index: 1500">
      <div class="w3-modal-content w3-card-4 w3-animate-zoom gdc-color-dark">
        <header class="w3-container w3-green"> 
          <h3>Briefing</h3>
          <span class="w3-button w3-xlarge w3-display-topright" onclick="document.getElementById('modalSuccess').style.display='none'">&times;</span>
        </header>
        <div id="contenuBriefing">

        </div>
        <footer id ="footer" class="w3-container w3-padding w3-khaki">
          <button class="w3-button w3-left w3-round w3-white w3-border" onclick="document.getElementById('modalBriefing').style.display='none';">Annuler</button>
        </footer>
    </div>
  </div>

    <link rel="stylesheet" href="https://unpkg.com/tabulator-tables@4.5.3/dist/css/tabulator.min.css" >
    <script src="https://unpkg.com/tabulator-tables@4.5.3/dist/js/tabulator.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.23.0/moment.min.js"></script>
    <script src="/javascript/listMissions.js"></script>  

  {{> footer}}   
</body>